int main()
{
    int verh_gran = pow(10, 6);

    for (int tek_chislo = 1; tek_chislo <= verh_gran; tek_chislo++)
        if (pr_na_palindr(tek_chislo))
            if (pr_na_palindr(perevod_v_dvoich(tek_chislo)))
                cout<<tek_chislo<<" "<< perevod_v_dvoich(tek_chislo)<< endl;
  
   
    ispolsuyu_ci_stroku();

}
