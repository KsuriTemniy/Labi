#include<iostream>
#include<math.h>

using namespace std;

////int pow(long long a, int b);

int perevod_v_dvoich(int chislo)
{
int dvoich = 0, k = 1;
    while (chislo)
    {
        dvoich += (chislo % 2) * k;
        k *= 10;
        chislo /= 2;
    }
    return dvoich;
}

bool pr_na_palindr(int tek_chislo)
{
    bool palindr = false;
    int temp_tek_chislo = tek_chislo;
    int temp_temp_tek_chislo = 0;

    while (temp_tek_chislo != 0)
    {
        temp_temp_tek_chislo = temp_temp_tek_chislo * 10 + temp_tek_chislo % 10;
        temp_tek_chislo /= 10;
    }
    return (temp_temp_tek_chislo == tek_chislo);
}

void ispolsuyu_ci_stroku()
{
    char* ci_stroka = new char[7];
    ci_stroka[0] = 'g';
    ci_stroka[1] = 'o';
    ci_stroka[2] = 'o';
    ci_stroka[3] = 'd';
    ci_stroka[4] = ' ';
    ci_stroka[5] = 'b';
    ci_stroka[6] = 'y';
    ci_stroka[7] = 'e';
    for (int i = 0; i <= 7; i++)
        cout << ci_stroka[i];
}

