#include<iostream>
#include<math.h>
using namespace std;
int main()
{
    for (int i = 1000; i<10000; i++)
    {
        bool flag = 0;
        int k = 2;
        int i1 = i/1000;
        int i2 = i%1000/100;
        int i3 = i%100/10;
        int i4 = i%10;
        if ((i1+i2) == (i3+i4))
            {
            while ((k<i)&&(flag == 0))
            {
                if ((i%k)==0)
                flag = 1;
                k++;
            }
            if (!flag) cout<<i<<endl;
            }
    
    }
}
